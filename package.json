    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.4.5",
    "extract-text-webpack-plugin": "2.0.0-rc.2",
    "html-webpack-plugin": "^2.26.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.1.0",
    "normalize.css": "^5.0.0",
    "offline-plugin": "^4.6.1",
    "preact": "^7.2.0",
    "preact-compat": "^3.13.1",
    "leaflet": "^1.0.2",
    "react-icons": "^2.2.1",
